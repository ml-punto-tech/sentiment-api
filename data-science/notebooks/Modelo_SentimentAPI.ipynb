        "    print(f\"   Vac√≠os/Nulos: {contador['vacios_nulos']}\")\n",
        "    print(f\"   Total Eliminados: {contador['total_eliminados']}\")\n",
        "\n",
        "    return df, contador, stats\n",
        "\n",

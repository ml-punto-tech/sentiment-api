        "    }\n",
        "\n",
        "    # GUARDAR EN EL DATAFRAME (NUEVO)\n",
        "    df.estadisticas_limpieza = stats\n",
        "\n",
        "    # Mostrar contador acumulado\n",
        "    print(f\"\\nüìà CONTADOR ACUMULADO:\")\n",

        ",\"tampoco\",\"pero\",\"sin\",\"mas\",\"muy\",\"mucho\",\"bien\",\"mal\",\"si\",\"contra\"}\n",
        "stop_words = stop_words - palabras_a_mantener\n",
        "def limpiar_texto(texto):\n",
        "    texto = texto.lower() # Min√∫sculas\n",
        "    texto = re.sub(r'[^\\w\\s]', '', texto) # Eliminar puntuacion\n",
        "    texto = \" \".join([word for word in texto.split() if word not in stop_words]) # Quitar stopwords\n",
        "    return texto\n",
        "\n",
        "# Aplicar a tu dataset\n",
        "df['texto'] = df['texto'].apply(limpiar_texto)"
